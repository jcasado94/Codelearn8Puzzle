    <meta charset="utf-8">
    <link rel="canonical" href="https://www.diariogol.com/fcbarcelona/coutinho-tiene-el-primer-lio-en-el-barca-y-es-muy-feo_532817_102.html">
    <meta property="op:markup_version" content="v1.0">

	<meta property="fb:use_automatic_ad_placement" content="false">

	
