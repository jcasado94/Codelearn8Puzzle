    <meta charset="utf-8">
    <link rel="canonical" href="https://www.diariogol.com/mas-futbol/shakira-tiene-un-plan-de-fuga-de-espana-con-o-sin-pique_532934_102.html">
    <meta property="op:markup_version" content="v1.0">

	<meta property="fb:use_automatic_ad_placement" content="false">

	
